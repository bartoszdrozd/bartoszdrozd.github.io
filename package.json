{
  "name": "Bartosz Drozd Portfolio",
  "version": "2.0.0",
  "description": "Modern portfolio with blog",
  "scripts": {
    "dev": "npx tailwindcss -i ./static/css/input.css -o ./static/css/output.css --watch",
    "build": "npx tailwindcss -i ./static/css/input.css -o ./static/css/output.css --minify",
    "serve": "python3 routes.py debug",
    "build:static": "python3 routes.py build"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0"
  }
}
