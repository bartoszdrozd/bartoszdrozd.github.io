{
  "name": "bartosz-drozd-portfolio",
  "version": "2.0.0",
  "description": "Modern portfolio with blog",
  "scripts": {
    "dev": "npx tailwindcss -i ./app/static/css/input.css -o ./app/static/css/output.css --watch",
    "build": "npx tailwindcss -i ./app/static/css/input.css -o ./app/static/css/output.css --minify",
    "serve": "flask --app app run --debug",
    "build:static": "flask --app app freeze"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0"
  }
}
